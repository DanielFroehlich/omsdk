                },
                "RAIDresetConfig": {
                    "default": "False",
                    "longDescription": "This pseudo attribute represents whether a reset config operation needs to be performed on the RAID controller.Reset Config operation deletes all the virtual disks that exists on the RAID controller. Configurable via XML configuration feature only.",
                    "qualifier": "Controller",
                    "readonly": "false",
                    "type": "RAIDresetConfigTypes"
                },
                "RAIDspinDownIdleTime": {
                    "longDescription": "Spindown idle time is the amount of time in seconds an uncofigured or hotspare can be idle before being spinned down by the controller.",
